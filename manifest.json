{
    "name": "Atli's dumb extension",
    "description": "A chrome extension intended to add small conveniences to chrome",
    "manifest_version": 2,
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "permissions": [
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "defualt_title": "Web Customizer"
    },
    "chrome_url_overrides" : {
        "newtab": "newtab.html"
    }
}
